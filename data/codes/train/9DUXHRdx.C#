using System;

class Sections
{
		static void Main()
	{
		/* int [] section = new int [];
		int [] students = new int [];
		string names = new string[];

		Console.Write("Enter number of Sections: ");
		int SecNum = int.Parse(Console.ReadLine());
		section = new int[SecNum];
		for(int i = 0; i < SecNum; i++)
