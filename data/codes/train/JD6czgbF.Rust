fn p2()->i64 {
    /* Prints out the sum of all multiples of 3 or 5 under 1000*/
    let mut i:i64 = 0;
    println!("Multiple of 3/5 : Sum of all multiples");
    for x in 0..1000 {
        if x%3==0 || x%5==0 {
            i+=x;
            println!("{}:{}",x,i);
        }
    }
    i
}